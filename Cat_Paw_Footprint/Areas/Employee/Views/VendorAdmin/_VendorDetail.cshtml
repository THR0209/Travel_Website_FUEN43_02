@model Cat_Paw_Footprint.Areas.Employee.ViewModel.VendorAdminViewModel
@{
    Layout = null;
}
<div class="container-fluid d-flex justify-content-center">
    <div class="col-md-8">
        <table class="table table-sm table-bordered">
            <tr><th>廠商編號</th><td>@Model.Account</td></tr>
            <tr><th>廠商名</th><td>@Model.CompanyName</td></tr>
            <tr><th>聯絡人</th><td>@Model.ContactName</td></tr>
            <tr><th>聯絡電話</th><td>@Model.Phone</td></tr>
            <tr><th>聯絡Email</th><td>@Model.Email</td></tr>
            <tr><th>地址</th><td>@Model.Address</td></tr>
            <tr><th>統一編號</th><td>@Model.TaxId</td></tr>
            <tr><th>建立時間</th><td>@Model.CreateDate.ToString("yyyy-MM-dd HH:mm")</td></tr>
            
        </table>
    </div>
</div>